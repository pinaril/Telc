<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE en-note SYSTEM "http://xml.evernote.com/pub/enml2.dtd">
<en-note style="margin:10px;font-size:16px;text-align:center;font-family:Georgia, serif;background-image: url(data:image/png;base64,)">
  
	<div style="font-size:16px;color:#5f5f5f;text-align:center;font-family:Georgia, serif;x-evernote:food-meal;margin:0px;padding:0px;min-width:290px">
    
		<div style="max-width:600px;padding-bottom:3.125%;margin:20px auto;">
			<div style="text-align:left;">
        <div style="padding-top:3.0%;padding-left:1.60%;padding-right:1.60%;">

          <!-- DATE -->
          
          <span style="padding:0;margin-top:0px;margin-bottom:22px;display:block;font-family:Helvetica;text-transform:uppercase;font-size:13px;color:#a09c96">
            {{dateLong}}
          </span>
          
          <h1 style="font-family:Georgia, serif;margin-top:4px;margin-bottom:1px;font-weight:normal;font-size:30px;color:#6692a0;word-wrap:break-word">
            <span style="x-evernote:title">
              {{mealTitleAppropriateForEvernoteUsage}}
            </span>
          </h1>
          
          <div style="clear:both;border-bottom-style:solid;border-color:#d0d0d0;border-width:thin;display:inline-block;margin:0px 0px 0px 0px;padding-bottom:6px;width:100%;">
            
            <span style="display;block;margin-bottom:0px;font-size:14px;font-family:Helvetica; font-weight:bold; color:#82a9b2;">

              {{#mealPlace.name}}

              {{#restaurantDetails}} {{#url}} <a style="text-decoration:none;color:rgb(39, 39, 39);" href="{{url}}"> {{/url}} {{/restaurantDetails}}
              
              <span style="margin-top:6px;margin-bottom:4px;display:inline-block;x-evernote:cuisine;text-align:left">
                <span style="display:inline-block;x-evernote:cuisine">
                  <span style="vertical-align:middle;display:inline-block;min-width:32px;min-height:32px;background-repeat:no-repeat;background-position:center -4px;background-image:url(data:image/png;base64,{{cuisineIconBase64PNG}})">
                  </span>
                </span>
                
                <span style="display:inline-block;font-family:Helvetica;font-size:16px;font-weight:bold;color:#76736e">{{mealPlace.name}}
                </span>
              </span>
                
              {{#restaurantDetails}}{{#url}}</a>{{/url}}{{/restaurantDetails}}
              
              {{/mealPlace.name}}

            </span>
          </div>

          <p style="margin-bottom:28px;margin-top:24px;display:block;clear:both;line-height:22px;font-family:Georgia, serif;font-weight:normal;font-size:16px;color:#4c4848;text-align:left">
            <span style="x-evernote:meal-review">{{{mealDescriptionAsHtml}}}</span>
          </p>
          
          {{#images}}
          
          <div style="padding: 0px 0px 0px 0px;margin-bottom:24px;">
            
            <span style="padding:1.50%;display:block;background-color:white;background-color:#ffffff;box-shadow:0px 1px 2px rgba(0,0,0,.5);-webkit-box-shadow:0px 1px 2px rgba(0,0,0,.5);x-evernote:caption-elem">
              <en-media style="margin-top:0.0px;margin-bottom:0px;display:block;width:100%;height:auto;x-evernote:caption-resource" hash="{{imageMD5Hash}}" type="image/jpeg" />              
              <p style="font-family:Georgia, serif;font-weight:normal;line-height:22px; color:#646464; font-size:15px; white-space: wrap;display:block;padding:6px 4px 0px 4px;margin:0px">
                <span style="x-evernote:caption-text">
                  {{caption}}
                </span>
              </p>
              
            </span>
          </div>
          {{/images}}
        </div>
			</div>
		</div>
	</div>
</en-note>